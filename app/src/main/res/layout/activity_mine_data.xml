<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="www.cityguestsociety.com.fourthfragmentactivity.MineDataActivity">
    <RelativeLayout
        android:layout_marginTop="@dimen/margin_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/userPhotoRelative"
                android:layout_width="match_parent"
                android:layout_height="65dp">
                <TextView
                    android:layout_marginLeft="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:textColor="@color/balck"
                    android:text="头像"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <www.cityguestsociety.com.ui.CircleImageView
                    android:src="@drawable/icon_head_portrait"
                    android:id="@+id/userPhoto"
                    android:layout_marginRight="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="@dimen/margin_40"
                    android:layout_height="@dimen/margin_40"/>
            </RelativeLayout>
            <TextView
                android:layout_marginLeft="@dimen/margin_15"
                android:background="@color/black_gray"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_1"/>
            <RelativeLayout
                android:id="@+id/userNickNameRelative"
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_marginLeft="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:textColor="@color/balck"
                    android:text="昵称"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/userNickName"
                    android:layout_marginRight="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
            <TextView
                android:layout_marginLeft="@dimen/margin_15"
                android:background="@color/black_gray"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_1"/>
            <RelativeLayout
                android:id="@+id/userPhoneNumberRelative"
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_marginLeft="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:textColor="@color/balck"
                    android:text="手机号"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/userNumber"
                    android:layout_marginRight="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
            <TextView
                android:layout_marginLeft="@dimen/margin_15"
                android:background="@color/black_gray"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_1"/>
            <RelativeLayout
                android:id="@+id/userSexRelative"
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_marginLeft="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:textColor="@color/balck"
                    android:text="性别"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/userSex"
                    android:layout_marginRight="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
            <TextView
                android:layout_marginLeft="@dimen/margin_15"
                android:background="@color/black_gray"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_1"/>
            <RelativeLayout
                android:id="@+id/userBorthRelative"
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_marginLeft="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:textColor="@color/balck"
                    android:text="出生日期"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/userBorth"
                    android:layout_marginRight="@dimen/margin_15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
